<Window x:Class="WpfApp5.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="WPF App" Height="800" Width="1200">

    <Grid Background="DarkGray">
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>

        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
            <Button Content="Klienci" Click="ButtonPage1_Click" Height="100" Width="200" Background="gray" FontFamily="Courier New" FontSize="24"/>
            <Button Content="Pojazdy" Click="ButtonPage2_Click" Height="100" Width="200" Background="gray" FontFamily="Courier New" FontSize="24"/>
            <Button Content="Wypożyczenia" Click="ButtonPage4_Click" Height="100" Width="200" Background="gray" FontFamily="Courier New" FontSize="24"/>
            <Button Content="Pracownicy" Click="ButtonPage3_Click" Height="100" Width="200" Background="gray" FontFamily="Courier New" FontSize="24"/>
            <Button Content="Otwórz w Nowym Oknie" Click="OpenCurrentInNewWindowButton_Click" Height="100" Width="250" Background="LightBlue" FontFamily="Courier New" FontSize="24" Margin="10"/>
        </StackPanel>

        <Frame x:Name="MainFrame" Grid.Row="1" NavigationUIVisibility="Visible" Cursor="Arrow"/>
    </Grid>
</Window>
